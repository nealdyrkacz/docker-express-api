{
  "name": "docker-express-api",
  "version": "1.0.0",
  "description": "A boilerplate Express API with Docker",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "start": "npm run tsc && node ./build/server.js",
    "tsc": "rm -r -f ./build && tsc",
    "dev": "npm run server:start",
    "server:start": "(ts-node-dev --transpileOnly ./src/server.ts)"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/helmet": "0.0.44",
    "@types/node": "^12.7.12",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-node-dev": "^1.0.0-pre.43"
  },
  "dependencies": {
    "@admin-bro/express": "^3.0.0",
    "@admin-bro/sequelize": "^1.1.0",
    "@hapi/joi": "^17.1.0",
    "@types/bcrypt": "^3.0.0",
    "@types/hapi__joi": "^16.0.9",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/lodash": "^4.14.149",
    "@types/validator": "^13.1.0",
    "admin-bro": "^3.2.5",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "cluster": "^0.7.7",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "helmet": "^3.21.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "os": "^0.1.1",
    "pg": "^7.18.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.3.1",
    "sequelize-cli": "^6.2.0",
    "ts-jsonapi": "^2.1.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.9.6",
    "uuid": "^8.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nealdyrkacz/docker-express-api"
  },
  "keywords": [
    "node",
    "api",
    "express",
    "sequelize"
  ]
}
